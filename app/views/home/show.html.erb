
<h1>Listing users</h1>
 
<table>
  <tr>
    <th>name</th>
    <th>email</th>
    <th>phone_number</th>
    <th>created_at</th>
    <th>updated_at</th>
  </tr>
 
  <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= user.phone_number %></td>
      <td><%= user.created_at %></td>
      <td><%= user.updated_at %></td>
    </tr>
  <% end %>
</table>
